{
  "name": "user-management-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "chmod +x ./docker-entrypoint.sh && ./docker-entrypoint.sh",
    "start:server": "node server.js",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "node prisma/seed.js",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "db:setup": "prisma migrate deploy && prisma db seed",
    "local": "npm run db:setup && npm run dev",
    "local:clean": "rm -rf .next && npm run db:setup && npm run dev",
    "deploy": "rm -rf .next && next build && ./docker-entrypoint.sh",
    "setup:local": "./setup-local-db.sh",
    "start:dev": "./start-dev.sh",
    "start:prod": "./start-prod.sh",
    "railway:start": "chmod +x ./fix-and-run.sh && ./fix-and-run.sh",
    "fix:migrations": "rm -rf prisma/migrations && npx prisma migrate dev --name init"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/lab": "^7.0.0-beta.11",
    "@mui/material": "^7.0.2",
    "@prisma/client": "^6.6.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "date-fns": "^3.6.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.28",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-query": "^3.39.3",
    "swr": "^2.2.5",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.31",
    "prisma": "^6.6.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
